<view style="width:100%; background: none;">

  <swiper indicator-dots="true" autoplay="true" interval="2000" duration="500" style="background: white; height: 200px; width: 100%;">
    <swiper-item>
      <image src="cloud://cloud1-4g0xrnaw5773a4e2.636c-cloud1-4g0xrnaw5773a4e2-1316920009/home1.jpeg" mode='aspectFill' class="swiper-image" />
    </swiper-item>
    <swiper-item>
      <image src="cloud://cloud1-4g0xrnaw5773a4e2.636c-cloud1-4g0xrnaw5773a4e2-1316920009/home2.jpg" mode='aspectFill' class="swiper-image" />
    </swiper-item>
    <swiper-item>
      <image src="cloud://cloud1-4g0xrnaw5773a4e2.636c-cloud1-4g0xrnaw5773a4e2-1316920009/home3.jpg" mode='aspectFill' class="swiper-image" />
    </swiper-item>
    <swiper-item>
      <image src="cloud://cloud1-4g0xrnaw5773a4e2.636c-cloud1-4g0xrnaw5773a4e2-1316920009/home4.jpg" mode='aspectFill' class="swiper-image" />
    </swiper-item>
    <swiper-item>
      <image src="cloud://cloud1-4g0xrnaw5773a4e2.636c-cloud1-4g0xrnaw5773a4e2-1316920009/home5.jpg" mode='aspectFill' class="swiper-image" />
    </swiper-item>
    <swiper-item>
      <image src="cloud://cloud1-4g0xrnaw5773a4e2.636c-cloud1-4g0xrnaw5773a4e2-1316920009/home6.jpeg" mode='aspectFill' class="swiper-image" />
    </swiper-item>
  </swiper>

  <view class="form-group_label">💰 积分榜</view>
  <view class="form-group" style="box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.05), 0 6px 20px 0 rgba(0, 0, 0, 0.05);">
    <view class="form-cell">
      <view class="slide">
        <view class="title">{{userA}}的积分：{{creditA}}</view>
      </view>
    </view>
    <view class="form-cell">
      <view class="slide">
        <view class="title">{{userB}}的积分：{{creditB}}</view>
      </view>
    </view>
  </view>

  <view class="form-group_label">🐻 欢迎回来</view>
  <view class="form-group" style="box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.05), 0 6px 20px 0 rgba(0, 0, 0, 0.05);">
    <view class="form-cell">
      <view class="slide">
        <view class="title">欢迎！伟铭比和玲玲！</view>
      </view>
    </view>
    <view class="form-cell">
      <view class="slide">
        <view class="title">今天想做什么呢？</view>
      </view>
    </view>
  </view>
  <view bindtap="requestSubscribeMessage">接收服务通知</view>

</view>